def print_bin(n):#целые числа 
	k=128#делитель
	while k>0:#выполнения цикла если к больше 0 
		d=n//k#переменная 
		n=0#обозначаем n для последующих формул
		print(d , end="")#если не выполяется пишется 
		n=n % k#замена остатка
		n=k//2#делитель следующего числа 
		break#закрытия цикла
x=int(input("Введите число от 0 до 255: "))#спрашиваяем основную переменную
print_bin(x)#вывод 
